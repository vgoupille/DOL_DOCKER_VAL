{
  "name": "R_DOL_project",
  "build": {
    "dockerfile": "../docker/Dockerfile.Val_dev",
    "context": "..",
    "args": {
      "CPU": "${localEnv:CPU}"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "reditorsupport.r",
        "rdebugger.r-debugger",
        "posit.shiny",
        "quarto.quarto",
        "purocean.drawio-preview",
        "redhat.vscode-yaml",
        "yzhang.markdown-all-in-one",
        "ms-azuretools.vscode-docker",
        "ms-vscode-remote.remote-containers",
        "ms-python.python",
        "ms-toolsai.jupyter",
        "mechatroner.rainbow-csv",
        "esbenp.prettier-vscode",
        "ms-toolsai.jupyter-renderers",
        "ms-python.debugpy",
        "ms-python.vscode-pylance",
        "aaron-bond.better-comments",
        "KevinRose.vsc-python-indent",
        "tomoki1207.pdf",
        "ms-python.black-formatter",
        "ms-toolsai.datawrangler",
        "GrapeCity.gc-excelviewer",
        "mhutchie.git-graph",
        "GitHub.vscode-pull-request-github",
        "eamodio.gitlens",
        "oderwat.indent-rainbow",
        "VisualStudioExptTeam.vscodeintellicode",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "James-Yu.latex-workshop",
        "sumneko.lua",
        "christian-kohler.path-intellisense",
        "donjayamanne.python-extension-pack"
      ],
      "settings": {
        "jupyter.jupyterServerType": "local",
        "jupyter.alwaysTrustNotebooks": true,
        "jupyter.askForKernelRestart": false,
        "jupyter.debugJustMyCode": true,
        "jupyter.enablePythonKernel": true,
        "jupyter.enableRDebugger": true,
        "jupyter.sendSelectionToInteractiveWindow": true,
        "jupyter.interactiveWindowMode": "perFile",
        "jupyter.notebookFileRoot": "${workspaceFolder}",
        "jupyter.autoSaveInterval": 1,
        "jupyter.enhancedCellToolbar": true,
        "jupyter.showCellStatusBar": true,
        "jupyter.cellTags": {
          "notebookKernelJupyterNotebook": {
            "backgroundColor": "#4CAF50",
            "color": "#FFFFFF"
          }
        },
        "jupyter.serverURI": "http://localhost:8888",
        "jupyter.remoteServerURI": "http://localhost:8888",
        "jupyter.serverLaunchTimeout": 60,
        "jupyter.serverConnectTimeout": 60,
        "jupyter.serverShutdownTimeout": 60,
        "jupyter.serverMaxRetries": 3,
        "jupyter.serverRetryInterval": 10000,
        "jupyter.serverPreload": true,
        "jupyter.serverPreloadTimeout": 60,
        "jupyter.serverPreloadMaxRetries": 3,
        "jupyter.serverPreloadRetryInterval": 10000
      }
    }
  },
  "forwardPorts": [8888],
  "postCreateCommand": "chmod +x /workspaces/DOL_DOCKER_VAL/docker/setting_files/start_jupyter.sh",
  "postStartCommand": "/workspaces/DOL_DOCKER_VAL/docker/setting_files/start_jupyter.sh"
}
